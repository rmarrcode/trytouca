<mjml>
  <mj-head>
    <mj-title>{{ subject }}</mj-title>
    <mj-preview>Someone wants to join your team {{ teamName }}.</mj-preview>
    <mj-include path="./layout/attributes.mjml" />
  </mj-head>
  <mj-body>
    <mj-include path="./layout/header.mjml" />
    <mj-section mj-class="wsl-border">
      <mj-column>
        <mj-text>
          User
          <b>{{ userName }}</b>
          is asking to join team
          <b>{{ teamName }}</b>
          .
        </mj-text>
        <mj-button mj-class="wsl-bg-color" href="{{ teamLink }}">
          Manage Request
        </mj-button>
      </mj-column>
    </mj-section>
    <mj-include path="./layout/footer-admin.mjml" />
  </mj-body>
</mjml>
